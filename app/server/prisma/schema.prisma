generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Jsonresults {
  id        String   @id @db.VarChar(255) // 对应 VARCHAR(255) 主键
  result    Json? // JSON 类型，允许为 null
  createdAt DateTime @default(now())
}
